{
  "prefix": "ng-pipe",
  "body": [
    "import { Pipe, PipeTransform } from '@angular/core';",
    "",
    "@Pipe({ name: '${NAME}' })",
    "export class ${NAME_PASCAL}Pipe implements PipeTransform {",
    "  transform(value: any, ...args: any[]) {",
    "    return value;",
    "  }",
    "}"
  ],
  "description": "Angular Pipe boilerplate"
}
